/// Temperature: convert Celsius(°C) to Fahrenheit(°F)
function 'DaxLib.Convert.CelsiusToFahrenheit' =
		(
		    // The temperature in Celsius
		    temperature: DOUBLE
		) =>
		    ( temperature * ( 9 / 5 ) ) + 32

	annotation DAXLIB_PackageId = DaxLib.Convert

	annotation DAXLIB_PackageVersion = 0.1.1-beta

/// Temperature: convert Celsius(°C) to Kelvin(K)
function 'DaxLib.Convert.CelsiusToKelvin' =
		(
		    // The temperature in Celsius
		    temperature: DOUBLE
		) =>
		    temperature + 273.15

	annotation DAXLIB_PackageId = DaxLib.Convert

	annotation DAXLIB_PackageVersion = 0.1.1-beta

