/// Time: convert human-readable time to epoch(Unix time)
function 'DaxLib.Convert.DateToEpoch' =
		(
		    // The date to convert
		    dateTime : DATETIME
		) =>
		    DATEDIFF ( DATE ( 1970, 1, 1 ), dateTime, SECOND )

	annotation DAXLIB_PackageId = DaxLib.Convert

	annotation DAXLIB_PackageVersion = 0.1.1-beta

/// Time: convert epoch(Unix time) to human-readable time
function 'DaxLib.Convert.EpochToDate' =
		(
		    // The epoch to convert
		    epoch: INT64
		) =>
		    DATE ( 1970, 1, 1 ) + ( epoch / 86400 )

	annotation DAXLIB_PackageId = DaxLib.Convert

	annotation DAXLIB_PackageVersion = 0.1.1-beta

/// Temperature: convert Fahrenheit(°F) to Celsius(°C)
function 'DaxLib.Convert.FahrenheitToCelsius' =
		(
		    // The temperature in Fahrenheit
		    temperature: DOUBLE
		) =>
		    ( temperature - 32 ) / ( 9 / 5 )

	annotation DAXLIB_PackageId = DaxLib.Convert

	annotation DAXLIB_PackageVersion = 0.1.1-beta

/// Temperature: convert Fahrenheit(°F) to Kelvin(K)
function 'DaxLib.Convert.FahrenheitToKelvin' =
		(
		    // The temperature in Fahrenheit
		    temperature: DOUBLE
		) =>
		    ( temperature + 459.67 ) * ( 5 / 9 )

	annotation DAXLIB_PackageId = DaxLib.Convert

	annotation DAXLIB_PackageVersion = 0.1.1-beta

